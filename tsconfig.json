{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": false,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
  },
  "exclude": ["dist","node_modules"]
}
